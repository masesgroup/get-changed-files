{
  "name": "retrieve-changed-files",
  "description": "GitHub action that retrieves all changed files in a pull request or push.",
  "version": "2.0.0",
  "private": true,
  "author": "MASES s.r.l. (original author Jitterbit, Inc.)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masesgroup/retrieve-changed-files.git"
  },
  "homepage": "https://github.com/masesgroup/retrieve-changed-files#readme",
  "bugs": {
    "url": "https://github.com/masesgroup/retrieve-changed-files/issues"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "TypeScript",
    "JavaScript",
    "Get",
    "Changed",
    "Modified",
    "Diff",
    "Files"
  ],
  "main": "lib/main.js",
  "engines": {
    "node": ">=16.*.*"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/**/* lib/**/*",
    "format": "prettier --write **/*.ts",
	"format-check": "prettier --check \"{,!(node_modules)/**/}*.ts\"",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "yarn clean && yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@octokit/rest": "^19.0.5",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.52.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.3"
  }
}
